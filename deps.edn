{:paths     ["src"]
 :mvn/repos {"pipo-mvn" {:url "s3://pipo-mvn/releases"}}
 :deps      {org.clojure/clojure {:mvn/version "1.10.1"}
             instaparse          {:mvn/version "1.4.10"}}
 :aliases   {:dev    {:extra-paths ["test" "test-resources"]
                      :extra-deps  {eftest {:mvn/version "0.5.9"}}}

             :cljfmt {:main-opts  ["-m" "cljfmt.main"]
                      :extra-deps {lein-cljfmt {:mvn/version "0.6.6"}}}

             :prod   {}}}
